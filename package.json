{
  "name": "launchdarkly-nodeutils",
  "version": "0.0.0-semantically-released",
  "description": "NodeJs utils to manage LaunchDarkly SaaS Feature Flags via API",
  "main": "index.js",
  "scripts": {
    "build": "npm run lint && rimraf dist/ && babel ./src --out-dir dist/src --copy-files && babel ./test --out-dir dist/test --copy-files && babel ./*.js --out-dir dist/ --copy-files && cp swagger.yaml dist/",
    "commit": "./node_modules/.bin/git-cz",
    "lint": "node ./node_modules/.bin/eslint ./src ./test --fix --ext=.js --ignore-pattern node_modules/ --ignore-pattern dist/ --ignore-pattern coverage/ --quiet",
    "test": "npm run build && cross-env NODE_ENV=test nyc ./node_modules/.bin/mocha --timeout 10000 --require babel-core/register --require babel-polyfill './dist/test/**/*.spec.js' && npm run jsdoc2markdown",
    "semantic-release": "semantic-release",
    "jsdoc2markdown": "node ./renderApiDocs.js"
  },
  "author": "Andrew Vaughan <andrewv@smartcreations.com.au> (https://halt.sh)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wyvern8/launchdarkly-nodeutils.git"
  },
  "keywords": [
    "launchdarkly",
    "featureflags"
  ],
  "bugs": {
    "url": "https://github.com/wyvern8/launchdarkly-nodeutils/issues"
  },
  "homepage": "https://github.com/wyvern8/launchdarkly-nodeutils#readme",
  "dependencies": {
    "app-root-path": "^2.0.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "bunyan": "^1.8.12",
    "bunyan-format": "^0.2.1",
    "fast-json-patch": "^2.0.6",
    "globule": "^1.2.0",
    "js-yaml": "^3.10.0",
    "json-format": "^1.0.1",
    "lodash": "^4.17.5",
    "swagger-client": "^3.4.7",
    "webpack": "^4.0.0"
  },
  "engines": {
    "node": ">=8.4.0"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-transform-async-to-generator": "^6.24 .1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "commitizen": "^2.9.6",
    "cross-env": "^5.1.3",
    "cz-conventional-changelog": "^2.1.0",
    "dotenv": "^5.0.0",
    "eslint": "^4.15.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "firstline": "^1.2.1",
    "format-json": "^1.0.3",
    "jsdoc": "^3.5.5",
    "jsdoc-to-markdown": "^4.0.1",
    "mocha": "^5.0.0",
    "nock": "^9.1.6",
    "node-sass": "^4.7.2",
    "nyc": "^11.4.1",
    "prettier": "1.10.2",
    "rimraf": "^2.6.2",
    "semantic-release": "^12.2.2",
    "serverless": "^1.25.0",
    "serverless-dotenv-plugin": "^1.0.1",
    "serverless-webpack": "^4.2.0",
    "sinon": "^4.1.3",
    "sqs-producer": "^1.6.2",
    "supertest": "^3.0.0"
  }
}
